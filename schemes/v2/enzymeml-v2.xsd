<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <!-- Roots -->
    <xs:element name="EnzymeMLDocument" type="EnzymeMLDocumentType"/>
    <xs:element name="Creator" type="CreatorType"/>
    <xs:element name="Vessel" type="VesselType"/>
    <xs:element name="Protein" type="ProteinType"/>
    <xs:element name="Complex" type="ComplexType"/>
    <xs:element name="SmallMolecule" type="SmallMoleculeType"/>
    <xs:element name="Reaction" type="ReactionType"/>
    <xs:element name="ReactionElement" type="ReactionElementType"/>
    <xs:element name="ModifierElement" type="ModifierElementType"/>
    <xs:element name="Equation" type="EquationType"/>
    <xs:element name="Variable" type="VariableType"/>
    <xs:element name="Parameter" type="ParameterType"/>
    <xs:element name="Measurement" type="MeasurementType"/>
    <xs:element name="MeasurementData" type="MeasurementDataType"/>
    <xs:element name="UnitDefinition" type="UnitDefinitionType"/>
    <xs:element name="BaseUnit" type="BaseUnitType"/>

    <!-- EnzymeMLDocument Definition -->
    <xs:complexType name="EnzymeMLDocumentType">
        <xs:sequence>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Title of the EnzymeML Document.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="version" type="xs:string" default="2.0">
                <xs:annotation>
                    <xs:documentation>
                        The version of the EnzymeML Document.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Description of the EnzymeML Document.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="created" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Date the EnzymeML Document was created.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="modified" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Date the EnzymeML Document was modified.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="creators">
                <xs:annotation>
                    <xs:documentation>
                        Contains descriptions of all authors that are part of the experiment.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Creator" type="CreatorType" maxOccurs="unbounded" minOccurs="1"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="vessels">
                <xs:annotation>
                    <xs:documentation>
                        Contains descriptions of all vessels that are part of the experiment.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Vessel" type="VesselType" maxOccurs="unbounded" minOccurs="1"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="proteins">
                <xs:annotation>
                    <xs:documentation>
                        Contains descriptions of all proteins that are part of the experiment
                        that may be referenced in reactions,
                        measurements, and equations.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Protein" type="ProteinType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="complexes">
                <xs:annotation>
                    <xs:documentation>
                        Contains descriptions of all complexes that are part of the experiment
                        that may be referenced in reactions,
                        measurements, and equations.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Complex" type="ComplexType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="small_molecules">
                <xs:annotation>
                    <xs:documentation>
                        Contains descriptions of all reactants that are part of the experiment
                        that may be referenced in reactions,
                        measurements, and equations.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="SmallMolecule" type="SmallMoleculeType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="reactions">
                <xs:annotation>
                    <xs:documentation>
                        Contains descriptions of all reactions that are part of the
                        experiment.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Reaction" type="ReactionType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="measurements">
                <xs:annotation>
                    <xs:documentation>
                        Contains descriptions of all measurements that are part of the
                        experiment.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Measurement" type="MeasurementType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="equations">
                <xs:annotation>
                    <xs:documentation>
                        Contains descriptions of all equations that are part of the
                        experiment.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Equation" type="EquationType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="parameters">
                <xs:annotation>
                    <xs:documentation>
                        Contains descriptions of all parameters that are part of the
                        experiment and may be used in equations.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Parameter" type="ParameterType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="references" type="xs:string"  maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Contains references to publications, databases, and arbitrary links to
                        the web.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Creator Definition -->
    <xs:complexType name="CreatorType">
        <xs:sequence>
            <xs:element name="given_name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Given name of the author or contributor.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="family_name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Family name of the author or contributor.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="mail" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Email address of the author or contributor.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Vessel Definition -->
    <xs:complexType name="VesselType">
        <xs:sequence>
            <xs:element name="id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Unique identifier of the vessel.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Name of the used vessel.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="volume" type="xs:float">
                <xs:annotation>
                    <xs:documentation>
                        Volumetric value of the vessel.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="unit" type="UnitDefinitionType">
                <xs:annotation>
                    <xs:documentation>
                        Volumetric unit of the vessel.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="constant" type="xs:boolean" default="true">
                <xs:annotation>
                    <xs:documentation>
                        Whether the volume of the vessel is constant or not. Default is True.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Protein Definition -->
    <xs:complexType name="ProteinType">
        <xs:sequence>
            <xs:element name="id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Identifier of the protein, such as a UniProt ID, or a custom
                        identifier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Name of the protein.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="constant" type="xs:boolean" default="true">
                <xs:annotation>
                    <xs:documentation>
                        Whether the concentration of the protein is constant through the
                        experiment or not. Default is True.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sequence" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Amino acid sequence of the protein
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="vessel_id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Identifier of the vessel this protein has been applied to.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ecnumber" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        EC number of the protein.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="organism" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Expression host organism of the protein.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="organism_tax_id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Taxonomy identifier of the expression host.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="references" type="xs:string"  maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        List of references to publications, database entries, etc. that
                        describe or reference the protein.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Complex Definition -->
    <xs:complexType name="ComplexType">
        <xs:sequence>
            <xs:element name="id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Unique identifier of the complex.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Name of the complex.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="constant" type="xs:boolean" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Whether the concentration of the complex is constant through the
                        experiment or not. Default is False.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="vessel_id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Unique identifier of the vessel this complex has been used in.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="participants" type="xs:string"  maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Array of IDs the complex contains
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- SmallMolecule Definition -->
    <xs:complexType name="SmallMoleculeType">
        <xs:sequence>
            <xs:element name="id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Identifier of the small molecule, such as a Pubchem ID, ChEBI ID, or a
                        custom identifier.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Name of the small molecule.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="constant" type="xs:boolean" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Whether the concentration of the small molecule is constant through
                        the experiment or not. Default is False.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="vessel_id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Identifier of the vessel this small molecule has been used in.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="canonical_smiles" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Canonical Simplified Molecular-Input Line-Entry System (SMILES)
                        encoding of the small molecule.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="inchi" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        International Chemical Identifier (InChI) encoding of the small
                        molecule.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="inchikey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Hashed International Chemical Identifier (InChIKey) encoding of the
                        small molecule.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="synonymous_names" type="xs:string"  maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        List of synonymous names for the small molecule.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="references" type="xs:string"  maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        List of references to publications, database entries, etc. that
                        describe or reference the small molecule.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Reaction Definition -->
    <xs:complexType name="ReactionType">
        <xs:sequence>
            <xs:element name="id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Unique identifier of the reaction.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Name of the reaction.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="reversible" type="xs:boolean" default="false">
                <xs:annotation>
                    <xs:documentation>
                        Whether the reaction is reversible or irreversible. Default is False.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="kinetic_law" type="EquationType">
                <xs:annotation>
                    <xs:documentation>
                        Mathematical expression of the reaction.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="reactants">
                <xs:annotation>
                    <xs:documentation>
                        List of reactants that are part of the reaction.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ReactionElement" type="ReactionElementType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="products">
                <xs:annotation>
                    <xs:documentation>
                        List of products that are part of the reaction.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ReactionElement" type="ReactionElementType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="modifiers">
                <xs:annotation>
                    <xs:documentation>
                        List of reaction elements that are not part of the reaction but
                        influence it.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ModifierElement" type="ModifierElementType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- ReactionElement Definition -->
    <xs:complexType name="ReactionElementType">
        <xs:sequence>
            <xs:element name="species_id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Internal identifier to either a protein or reactant defined in the
                        EnzymeML Document.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="stoichiometry" type="xs:float" default="1.0">
                <xs:annotation>
                    <xs:documentation>
                        Float number representing the associated stoichiometry.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- ModifierElement Definition -->
    <xs:complexType name="ModifierElementType">
        <xs:sequence>
            <xs:element name="species_id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Internal identifier to either a protein or reactant defined in the
                        EnzymeML Document.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="role" type="ModifierRoleType">
                <xs:annotation>
                    <xs:documentation>
                        Role of the modifier in the reaction.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Equation Definition -->
    <xs:complexType name="EquationType">
        <xs:sequence>
            <xs:element name="species_id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Identifier of a defined species (SmallMolecule, Protein, Complex).
                        Represents the left hand side of the equation.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="equation" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Mathematical expression of the equation. Represents the right hand
                        side of the equation.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="equation_type" type="EquationTypeType">
                <xs:annotation>
                    <xs:documentation>
                        Type of the equation.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="variables">
                <xs:annotation>
                    <xs:documentation>
                        List of variables that are part of the equation
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Variable" type="VariableType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Variable Definition -->
    <xs:complexType name="VariableType">
        <xs:sequence>
            <xs:element name="id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Identifier of the variable.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Name of the variable.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="symbol" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Equation symbol of the variable.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Parameter Definition -->
    <xs:complexType name="ParameterType">
        <xs:sequence>
            <xs:element name="id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Identifier of the parameter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Name of the parameter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="symbol" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Equation symbol of the parameter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="value" type="xs:float">
                <xs:annotation>
                    <xs:documentation>
                        Numerical value of the estimated parameter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="unit" type="UnitDefinitionType">
                <xs:annotation>
                    <xs:documentation>
                        Unit of the estimated parameter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="initial_value" type="xs:float">
                <xs:annotation>
                    <xs:documentation>
                        Initial value that was used for the parameter estimation.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="upper_bound" type="xs:float">
                <xs:annotation>
                    <xs:documentation>
                        Upper bound for the parameter value that was used for the parameter
                        estimation
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="lower_bound" type="xs:float">
                <xs:annotation>
                    <xs:documentation>
                        Lower bound for the parameter value that was used for the parameter
                        estimation
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="stderr" type="xs:float">
                <xs:annotation>
                    <xs:documentation>
                        Standard error of the estimated parameter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="constant" type="xs:boolean" default="true">
                <xs:annotation>
                    <xs:documentation>
                        Specifies if this parameter is constant. Default is True.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- Measurement Definition -->
    <xs:complexType name="MeasurementType">
        <xs:sequence>
            <xs:element name="id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Unique identifier of the measurement.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Name of the measurement
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="species_data">
                <xs:annotation>
                    <xs:documentation>
                        Measurement data of all species that were part of the measurement.
                        A species refers to a Protein, Complex, or
                        SmallMolecule.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="MeasurementData" type="MeasurementDataType" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="group_id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        User-defined group ID to signal relationships between measurements.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ph" type="xs:float">
                <xs:annotation>
                    <xs:documentation>
                        pH value of the measurement.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="temperature" type="xs:float">
                <xs:annotation>
                    <xs:documentation>
                        Temperature of the measurement.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="temperature_unit" type="UnitDefinitionType">
                <xs:annotation>
                    <xs:documentation>
                        Unit of the temperature of the measurement.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <!-- MeasurementData Definition -->
    <xs:complexType name="MeasurementDataType">
        <xs:sequence>
            <xs:element name="species_id" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The identifier for the described reactant.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="prepared" type="xs:float">
                <xs:annotation>
                    <xs:documentation>
                        Amount of the the species before starting the measurement. This field
                        can be used for specifying the prepared amount
                        of a species in the reaction mix. Not to be
                        confused with , specifying the concentration
                        of a species at the first data point from
                        the array.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="initial" type="xs:float">
                <xs:annotation>
                    <xs:documentation>
                        Initial amount of the measurement data. This must be the same as the
                        first data point in the array.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="data_unit" type="UnitDefinitionType">
                <xs:annotation>
                    <xs:documentation>
                        SI unit of the data that was measured.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="data" type="xs:float"  maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Data that was measured.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
