@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix md: <http://mdmodel.net/> .
@prefix OBO: <http://purl.obolibrary.org/obo/> .
@prefix schema: <https://schema.org/> .


md:EnzymeMLDocumentShape
    a sh:NodeShape ;
    sh:targetClass md:EnzymeMLDocument ;

    sh:property [
        sh:path schema:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ];

md:CreatorShape
    a sh:NodeShape ;
    sh:targetClass md:Creator ;

    sh:property [
        sh:path schema:givenName ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ];
    sh:property [
        sh:path schema:familyName ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ];
    sh:property [
        sh:path schema:email ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ].

md:VesselShape
    a sh:NodeShape ;
    sh:targetClass md:Vessel ;

    sh:property [
        sh:path schema:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ];
    sh:property [
        sh:path OBO:OBI_0002139 ;
        sh:datatype xsd:double ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ];
    sh:property [
        sh:path schema:identifier ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
    ].

md:ProteinShape
    a sh:NodeShape ;
    sh:targetClass md:Protein ;

    sh:property [
        sh:path schema:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ];
    sh:property [
        sh:path OBO:GSSO_007262 ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ];
    sh:property [
        sh:path schema:identifier ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
    ];
    sh:property [
        sh:path OBO:OBI_0100026 ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
    ];

md:SmallMoleculeShape
    a sh:NodeShape ;
    sh:targetClass md:SmallMolecule ;

    sh:property [
        sh:path schema:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
    ];
    sh:property [
        sh:path schema:identifier ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
    ];